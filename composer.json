{
	"name": "cnvs/canvas",
	"description": "A Laravel publishing platform",
	"keywords": ["cnvs", "canvas", "laravel", "publishing"],
	"license": "MIT",
	"homepage": "https://cnvs.io",
	"support": {
		"issues": "https://github.com/cnvs/canvas/issues",
		"source": "https://github.com/cnvs/canvas"
	},
	"authors": [
		{
			"name": "Todd Austin",
			"email": "austin.todd.j@gmail.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.1.3",
		"laravel/framework": "~5.7",
		"nicolaslopezj/searchable": "1.*"
	},
	"require-dev": {
		"mockery/mockery": "^1.1",
		"orchestra/testbench": "~3.7.0",
		"phpunit/phpunit": "^7.4"
	},
	"autoload": {
		"psr-4": {
			"Canvas\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Canvas\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"config": {
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"providers": [
				"Canvas\\CanvasServiceProvider"
			]
		}
	}
}
